<h1 style="text-align: center">Factual Results</h1>
 
        <ul>
        <% @results.each do |result| %>
        <% puts result %>
        <% @name = result['product_name'] %>
        <% @url = result["image_urls"] %>
        <% @category = result["category"] %>

          <li> 

<%= link_to truncate(result['product_name'], length: 35, separator: ' '), results_show_path(:id => result['factual_id']) %>
          </li>
      <% end %>
      </ul>


 